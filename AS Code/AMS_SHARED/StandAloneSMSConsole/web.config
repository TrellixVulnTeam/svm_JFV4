<?xml version="1.0"?>
<configuration>
  <connectionStrings>
    <add name="DefaultConnectionString" connectionString="Provider=Microsoft.Jet.OLEDB.4.0;Data Source=C:\AdminStudio Shared\sample.mdb;" providerName="System.Data.OleDb"/>
  </connectionStrings>
  <system.web>
    <!--setting for foreign language-->
    <globalization requestEncoding="ISO-8859-1" responseEncoding="ISO-8859-1" fileEncoding="ISO-8859-1" />
    <!--Set http request length and execution timeout-->
    <httpRuntime maxRequestLength="614400" executionTimeout="2400" />
    <!-- This section is needed for the Web service to process Get and Post request-->
    <xhtmlConformance mode="Legacy" />
    <compilation debug="false">
      <compilers>
        <compiler language="c#" extension=".cs" type="Microsoft.CSharp.CSharpCodeProvider, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=B77A5C561934E089" compilerOptions="/d:DEBUG;TRACE" />
      </compilers>
      <assemblies>
        <add assembly="System.DirectoryServices, Version=2.0.0.0, Culture=neutral, PublicKeyToken=B03F5F7F11D50A3A" />
        <add assembly="System.Windows.Forms, Version=2.0.0.0, Culture=neutral, PublicKeyToken=B77A5C561934E089" />
        <add assembly="System.Design, Version=2.0.0.0, Culture=neutral, PublicKeyToken=B03F5F7F11D50A3A" />
        <add assembly="System.Management, Version=2.0.0.0, Culture=neutral, PublicKeyToken=B03F5F7F11D50A3A" />
        <add assembly="System.Data.OracleClient, Version=2.0.0.0, Culture=neutral, PublicKeyToken=B77A5C561934E089" />
        <add assembly="System.Configuration.Install, Version=2.0.0.0, Culture=neutral, PublicKeyToken=B03F5F7F11D50A3A" />
      </assemblies>
    </compilation>
    <!--Set the customized error page-->
    <customErrors mode="Off" defaultRedirect="AMSError.aspx" />
    <!--Set system session mode-->
    <sessionState timeout="20" mode="InProc" stateConnectionString="tcpip=127.0.0.1:42424" sqlConnectionString="data source=127.0.0.1;Trusted_Connection=yes" cookieless="false" />
  </system.web>
  <!--Customized system settings-->
  <appSettings>
    <add key="ASES_URL" value="http://localhost" />
    <add key="StandAloneSMSConsole" value="1" />
  </appSettings>
  <!--Web trace module log level: 0,1,2,3,4. 4 is the higest level, log everything-->
  <system.diagnostics>
    <switches>
      <add name="ISTraceSwitch" value="0" />
    </switches>
  </system.diagnostics>
</configuration>